<form method="post" action="{{ path('app_questions_delete', {'id': question.id}) }}" onsubmit="return confirm('Jesteś pewnien, że chcesz usunąć to pytanie z rozwiązaniem?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ question.id) }}">
    <button class="btn btn-outline-danger"><i class="fa fa-trash"></i></button>
</form>
